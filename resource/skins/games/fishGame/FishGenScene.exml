<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="FishGenSceneSkin" width="80" height="80" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing" xmlns:uiCore="uiCore.*" xmlns:ns1="*">	 
    <e:Image id="fishBoard" horizontalCenter="0" verticalCenter="0" source="board"/>   
	<e:Image id="fishGen" x="40" y="40" anchorOffsetX="43" anchorOffsetY="60"  source="gun1"/>
	<e:Image id="fishSeat" horizontalCenter="0" verticalCenter="0" source="seat"/>
	<e:Image id="fishgrade" horizontalCenter="0" verticalCenter="0" source="disc"/>
	<ns1:ParticleGroup id="LfireLight" particleScaleX="0.5"  particleScaleY="0.5" source="fireLight"></ns1:ParticleGroup>
	<ns1:ParticleGroup id="RfireLight" particleScaleX="0.5"  particleScaleY="0.5" source="fireLight"></ns1:ParticleGroup>
	<ns1:ParticleGroup id="LlittleLight" source="littleLight"></ns1:ParticleGroup>
	<ns1:ParticleGroup id="RlittleLight" source="littleLight"></ns1:ParticleGroup>
	<uiCore:Button id="addbtn" horizontalCenter="70" verticalCenter="10" visible="false">
			<uiCore:skinName>
				<e:Skin states="up,down,disabled">
					<e:Image scaleX="0.6%" scaleY="0.6%" source="add"/>
				</e:Skin>
			</uiCore:skinName> 
	</uiCore:Button>
	<uiCore:Button id="downbtn" horizontalCenter="-70" verticalCenter="10" visible="false">
