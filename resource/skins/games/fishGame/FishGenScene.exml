	<e:BitmapLabel id="oddsMoney" font="gunFont_fnt" text="0" scaleX="0.6%" scaleY="0.6%" horizontalCenter="0" verticalCenter="0" ></e:BitmapLabel>
	<e:Image id="moneybg" horizontalCenter="130" verticalCenter="-30" scaleX="0.6%" scaleY="0.6%" source="01" />	 
	<e:BitmapLabel id="money" font="gunGoldFont_fnt" text="0" scaleX="0.6%" scaleY="0.6%" horizontalCenter="130" verticalCenter="-30" ></e:BitmapLabel>
	<e:Label id="playername" text="亲爱的玩家" fontFamily="Microsoft YaHei" size="20" textColor="0xffffff" width="170" horizontalCenter="180" verticalCenter="10" textAlign="left"  verticalAlign="bottom"/>	
	<ns1:CircleImage id="card" scaleX="1%" scaleY="1%" x="-80" y="-140" source="card1" visible="false"/>
	<ns1:CircleGroup id="lockFishView" x="120" y="-180"  visible="false">
		<e:Image scaleX="0.6%" scaleY="0.6%" source="zzsd" /> 
	</ns1:CircleGroup>
